    <file src="bin/Release/FastAndroidCamera.dll" target="lib/MonoAndroid403" />
    <file src="bin/Release/FastAndroidCamera.xml" target="lib/MonoAndroid403" />
    <file src="*.cs" target="src" />
  </files>
</package>
